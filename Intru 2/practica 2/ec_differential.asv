function dy_dt = ec_differential(t,Y,E,u)
    global m c1 c2 c3 c4 Fs Fd Ad Kt R V 
    Xv1 = Y(1); %Desplazamiento
    Xv2 = Y(2); %Velocidad
    Pd = Y(3); %Presion dentro del vastago
    
        
    dXv1_dt = Xv2;
    dXv2_dt = (1/m)*(-c2*Xv2-c4*(Xv1)^3-(c1-c3)*Xv1-Fs-Fd+Ad*Pd);
    dPd_dt = ((u-Pd)*Kt)/(R*V);
   

    dy_dt = [dVc_dt; dIl_dt;dIc_dt;dVl_dt];

    

end